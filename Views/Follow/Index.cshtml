@model socset.ViewModels.FollowVM

<h2>Профиль @Model.User.UserName</h2>
<h3> Подписчики:</h3>
<ul>
    @foreach (var follower in Model.Followers)
    {
        <li>@follower.UserName</li>
    }
</ul>

<h3>Подписки:</h3>
<ul>
    @foreach (var followee in Model.Following)
    {
        <li>@followee.UserName</li>
    }
</ul>

